        <joint name="rod_uav_joint_1" type="revolute">
		    <pose>
		        0 0 -0.115 0 -1.57 0
		    </pose>
		    <child>first_cylinder</child>
		    <parent>fake_cylinder</parent>
		    <axis>
		        <limit>
		            <lower>-3.14</lower>
		            <upper>1.57</upper>
		        </limit>
		        <xyz>0 1 0</xyz>
                       <dynamics>  		
                       	<spring_stiffness>300.01</spring_stiffness>
		  		<spring_reference>0.01</spring_reference>
				<damping>5.5</damping>
				<friction>0.02</friction>
                       </dynamics>
		    </axis>
		    <physics>
		    	<provide_feedback>true</provide_feedback>
			<ode>
				<implicit_spring_damper>1</implicit_spring_damper>
				<cfm_damping>1</cfm_damping>
				<limit>
					<cfm>2.05</cfm>
					<erp>2.8</erp>
				</limit>
			</ode>
		    </physics>
	</joint>
		
		
		
		
