        <!-- Idea: creating an almost null-mass and dimension cylinder for a joint to be measured -->
        <link name='fake_cylinder'>
            <pose>0 0 0.005 0 1.57 0</pose>
			<inertial>
                <mass>0.00714285</mass>
                <inertia>
                    <ixx>0.00000024</ixx>
                    <ixy>0.000000</ixy>
                    <ixz>0.000000</ixz>
                    <iyy>0.00000024</iyy>
                    <iyz>0.000000</iyz>
                    <izz>0.00000024</izz>
                </inertia>
            </inertial>

            <collision name='fake_cylinder_inertia_collision'>
                <geometry>
                    <cylinder>
                        <radius>0.005</radius>
                        <length>0.01</length>
                    </cylinder>
                </geometry>
                <surface>
                    <contact>
                        <ode>
             				<min_depth>0.001</min_depth>
                            <max_vel>0</max_vel>
                        </ode>
                    </contact>
                    <friction>
                        <ode/>
                    </friction>
                </surface>
            </collision>

